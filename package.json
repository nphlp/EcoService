{
    "name": "eco-service",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "prepare": "husky || true",
        "dev": "next dev --turbopack",
        "build": "next build --turbopack",
        "build:analyze": "ANALYZE=true next build",
        "start": "next start",
        "auto": "pnpm install && pnpm db:setup && pnpm prisma:generate && pnpm prisma:deploy && pnpm fixtures:setup && pnpm dev",
        "auto:start": "pnpm install && pnpm db:setup && pnpm prisma:generate && pnpm prisma:deploy && pnpm fixtures:setup && pnpm build && pnpm start",
        "lint": "eslint",
        "lint:fix": "eslint . --fix",
        "format": "prettier . --check --log-level warn",
        "format:fix": "prettier . --write --log-level error",
        "type": "tsc --noEmit",
        "checks": "pnpm type && pnpm lint:fix && pnpm format:fix",
        "test:run": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "db:setup": "./scripts/db.sh setup",
        "db:reset": "./scripts/db.sh reset",
        "db:reload": "./scripts/db.sh reload",
        "db:execute": "./scripts/db.sh execute",
        "prisma:generate": "pnpm dlx prisma generate",
        "prisma:migrate": "pnpm dlx prisma migrate dev",
        "prisma:deploy": "pnpm dlx prisma migrate deploy",
        "prisma:reset": "pnpm dlx prisma migrate reset",
        "prisma:studio": "pnpm dlx prisma studio",
        "fixtures:setup": "tsx scripts/fixtures.ts setup",
        "fixtures:reset": "tsx scripts/fixtures.ts reset",
        "fixtures:reload": "tsx scripts/fixtures.ts reload",
        "stripe:webhooks": "stripe listen --forward-to localhost:3000/api/stripe/webhooks",
        "solid:list": "tsx solid/solid-generator.ts list",
        "solid:clear": "tsx solid/solid-generator.ts clear",
        "solid:all": "tsx solid/solid-generator.ts all"
    },
    "dependencies": {
        "@prisma/adapter-pg": "^7.1.0",
        "@prisma/client": "^7.1.0",
        "@stripe/react-stripe-js": "^5.4.1",
        "dayjs": "^1.11.19",
        "dotenv": "^17.2.3",
        "next": "16.1.0-canary.13",
        "pg": "^8.16.3",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "tsx": "^4.21.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.2.0",
        "@commitlint/config-conventional": "^20.2.0",
        "@commitlint/types": "^20.2.0",
        "@headlessui/react": "^2.2.9",
        "@hookform/resolvers": "^5.2.2",
        "@next/bundle-analyzer": "^16.0.10",
        "@orpc/client": "^1.12.2",
        "@orpc/openapi": "^1.12.2",
        "@orpc/server": "^1.12.2",
        "@orpc/zod": "^1.12.2",
        "@oslojs/encoding": "^1.1.0",
        "@react-email/components": "^1.0.1",
        "@react-email/render": "^2.0.0",
        "@scalar/api-reference-react": "^0.8.11",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^12.0.2",
        "@stripe/stripe-js": "^8.5.3",
        "@tailwindcss/postcss": "^4.1.18",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/react": "^16.3.0",
        "@trivago/prettier-plugin-sort-imports": "^6.0.0",
        "@types/leaflet": "^1.9.21",
        "@types/lodash": "^4.17.21",
        "@types/node": "^25.0.1",
        "@types/nodemailer": "^7.0.4",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.2",
        "@vitest/coverage-v8": "^4.0.15",
        "babel-plugin-react-compiler": "^1.0.0",
        "better-auth": "^1.4.6",
        "clsx": "^2.1.1",
        "eslint": "^9.39.1",
        "eslint-config-next": "^16.0.10",
        "eslint-plugin-react-refresh": "^0.4.24",
        "eslint-plugin-unused-imports": "^4.3.0",
        "handlebars": "^4.7.8",
        "husky": "^9.1.7",
        "jsdom": "^27.3.0",
        "leaflet": "^1.9.4",
        "lint-staged": "^16.2.7",
        "lodash": "^4.17.21",
        "lucide-react": "^0.561.0",
        "motion": "^12.23.26",
        "nanoid": "^5.1.6",
        "nodemailer": "^7.0.11",
        "nuqs": "^2.8.5",
        "postcss": "^8.5.6",
        "prettier": "^3.7.4",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "prisma": "^7.1.0",
        "react-leaflet": "^5.0.0",
        "semantic-release": "^25.0.2",
        "server-only": "^0.0.1",
        "stripe": "^20.0.0",
        "tailwind-merge": "^3.4.0",
        "tailwindcss": "^4.1.18",
        "tw-animate-css": "^1.4.0",
        "typescript": "^5.9.3",
        "typescript-cookie": "^1.0.6",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^4.0.15",
        "zod": "^4.1.13",
        "zustand": "^5.0.9"
    },
    "lint-staged": {
        "*.{js,ts,tsx}": "eslint --fix",
        "*.{js,ts,tsx,json,css,md,yml,yaml}": "prettier --write"
    }
}
