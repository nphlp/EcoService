{
    "name": "eco-service",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "tsx scripts/help.ts tip && next dev --turbopack",
        "build": "next build --turbopack",
        "start": "next start",
        "start:debug": "NODE_OPTIONS='--trace-warnings --enable-source-maps' NODE_ENV=development NEXTJS_SHOW_SERVER_ERRORS=true next start",
        "lint": "eslint",
        "lint:fix": "eslint . --fix",
        "lint:file": "eslint --fix",
        "format": "prettier . --check",
        "format:fix": "prettier . --write",
        "format:file": "prettier --write",
        "type": "tsc --noEmit",
        "checks": "pnpm run type && pnpm run lint && pnpm run format",
        "prepare": "husky || true",
        "test:watch": "vitest",
        "test:run": "vitest run",
        "test:server:dev": "tsx scripts/run-test-on-bg-server.ts",
        "test:server:prod": "tsx scripts/run-test-on-bg-server.ts --prod",
        "auto": "pnpm install && pnpm db:reload && pnpm prisma:generate && pnpm prisma:deploy && pnpm fixtures:reload && pnpm run dev",
        "hybrid": "dotenv -e docker/.env.override.hybrid -- sh -c 'pnpm install && pnpm db:reload --docker && pnpm prisma:generate && pnpm prisma:deploy && pnpm fixtures:reload && pnpm run dev'",
        "db:setup": "tsx scripts/db.ts setup",
        "db:reset": "tsx scripts/db.ts reset",
        "db:reload": "tsx scripts/db.ts reload",
        "db:execute": "tsx scripts/db.ts execute",
        "prisma:generate": "pnpm dlx prisma generate",
        "prisma:migrate": "pnpm dlx prisma migrate dev",
        "prisma:deploy": "pnpm dlx prisma migrate deploy",
        "prisma:reset": "pnpm dlx prisma migrate reset",
        "fixtures:setup": "tsx scripts/fixtures.ts setup",
        "fixtures:reset": "tsx scripts/fixtures.ts reset",
        "fixtures:reload": "tsx scripts/fixtures.ts reload",
        "stripe:webhooks": "stripe listen --forward-to localhost:3000/api/stripe/webhooks",
        "help": "tsx scripts/help.ts help",
        "generate:list": "tsx scripts/generator.ts list",
        "generate:clear": "tsx scripts/generator.ts clear",
        "generate:all": "tsx scripts/generator.ts all",
        "clear": "rm -rf next-env.d.ts prisma/client node_modules .next .husky/_"
    },
    "dependencies": {
        "@headlessui/react": "^2.2.8",
        "@prisma/client": "^6.16.1",
        "@stripe/react-stripe-js": "^3.10.0",
        "dotenv": "^17.2.2",
        "next": "15.6.0-canary.6",
        "prettier": "3.6.2",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "redis": "^5.8.2",
        "tsx": "^4.20.5"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@commitlint/types": "^19.8.1",
        "@eslint/eslintrc": "^3.3.1",
        "@next/eslint-plugin-next": "^15.5.3",
        "@oslojs/encoding": "^1.1.0",
        "@plunk/node": "^3.0.3",
        "@react-email/components": "0.3.2",
        "@react-email/render": "^1.2.3",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^11.0.6",
        "@stripe/stripe-js": "^7.9.0",
        "@tailwindcss/postcss": "^4.1.13",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/react": "^16.3.0",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/leaflet": "^1.9.20",
        "@types/lodash": "^4.17.20",
        "@types/node": "^24.4.0",
        "@types/react": "^19.1.13",
        "@types/react-dom": "^19.1.9",
        "@typescript-eslint/eslint-plugin": "^8.43.0",
        "@typescript-eslint/parser": "^8.43.0",
        "@vitejs/plugin-react": "^4.7.0",
        "better-auth": "1.3.1",
        "clsx": "^2.1.1",
        "dotenv-cli": "^8.0.0",
        "eslint": "^9.35.0",
        "eslint-config-next": "15.4.2",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "eslint-plugin-unused-imports": "^4.2.0",
        "handlebars": "^4.7.8",
        "husky": "^9.1.7",
        "jsdom": "^26.1.0",
        "leaflet": "^1.9.4",
        "lint-staged": "^16.1.6",
        "lodash": "^4.17.21",
        "lucide-react": "^0.525.0",
        "motion": "^12.23.12",
        "nanoid": "^5.1.5",
        "nuqs": "^2.6.0",
        "postcss": "^8.5.6",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "prisma": "^6.16.1",
        "react-easy-crop": "^5.5.0",
        "react-leaflet": "^5.0.0",
        "sass": "^1.92.1",
        "semantic-release": "^24.2.8",
        "server-only": "^0.0.1",
        "stripe": "^18.5.0",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.13",
        "tw-animate-css": "^1.3.8",
        "typescript": "^5.9.2",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4",
        "zod": "^4.1.8",
        "zustand": "^5.0.8"
    },
    "lint-staged": {
        "*.{js,ts,tsx}": "pnpm lint:file",
        "*.{js,ts,tsx,json,css,md,yml,yaml}": "pnpm format:file"
    }
}
