{
    "name": "eco-service",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "tsx scripts/help.ts tip && next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "start:debug": "NODE_OPTIONS='--trace-warnings --enable-source-maps' NODE_ENV=development NEXTJS_SHOW_SERVER_ERRORS=true next start",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "lint:file": "next lint --fix --file",
        "format": "pnpm exec prettier . --check --ignore-path .prettierignore",
        "format:fix": "pnpm exec prettier . --write --ignore-path .prettierignore",
        "format:file": "prettier --write --ignore-path .prettierignore",
        "type": "tsc --noEmit",
        "prepare": "husky || true",
        "test:watch": "vitest",
        "test:run": "vitest run",
        "auto": "pnpm install && pnpm db:reload root && pnpm prisma:generate && pnpm prisma:migrate && pnpm fixtures:reload && pnpm run dev",
        "db:setup": "tsx scripts/db.ts setup.sql",
        "db:reset": "tsx scripts/db.ts reset.sql",
        "db:reload": "tsx scripts/db.ts reload",
        "prisma:generate": "pnpm dlx prisma generate",
        "prisma:migrate": "pnpm dlx prisma migrate dev",
        "prisma:deploy": "pnpm dlx prisma migrate deploy",
        "prisma:reset": "pnpm dlx prisma migrate reset",
        "fixtures:setup": "tsx scripts/fixtures.ts setup",
        "fixtures:reset": "tsx scripts/fixtures.ts reset",
        "fixtures:reload": "tsx scripts/fixtures.ts reload",
        "stripe:webhooks": "stripe listen --forward-to localhost:3000/api/stripe/webhooks",
        "help": "tsx scripts/help.ts help",
        "generate:list": "tsx scripts/generator.ts list",
        "generate:clear": "tsx scripts/generator.ts clear",
        "generate:all": "tsx scripts/generator.ts all",
        "clear": "rm -rf next-env.d.ts prisma/zod prisma/client node_modules .next .husky/_"
    },
    "dependencies": {
        "@prisma/client": "^6.6.0",
        "@stripe/react-stripe-js": "^3.7.0",
        "next": "15.4.0-canary.39",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "tsx": "^4.19.3"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "^19.8.0",
        "@eslint/eslintrc": "^3.3.1",
        "@next/eslint-plugin-next": "^15.3.1",
        "@oslojs/encoding": "^1.1.0",
        "@plunk/node": "^3.0.3",
        "@radix-ui/react-dialog": "^1.1.10",
        "@radix-ui/react-popover": "^1.1.10",
        "@radix-ui/react-slot": "^1.2.0",
        "@react-email/components": "0.0.33",
        "@react-email/render": "^1.0.6",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^11.0.2",
        "@stripe/stripe-js": "^6.1.0",
        "@tailwindcss/postcss": "^4.1.4",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/react": "^16.3.0",
        "@types/leaflet": "^1.9.17",
        "@types/lodash": "^4.17.16",
        "@types/node": "^22.14.1",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.2",
        "@typescript-eslint/parser": "^8.30.1",
        "@vitejs/plugin-react": "^4.4.1",
        "better-auth": "1.2.3",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "cmdk": "^1.1.1",
        "eslint": "^9.25.0",
        "eslint-config-next": "15.3.1",
        "eslint-plugin-react-hooks": "^5.2.0",
        "framer-motion": "^12.7.4",
        "handlebars": "^4.7.8",
        "husky": "^9.1.7",
        "jsdom": "^26.1.0",
        "leaflet": "^1.9.4",
        "lint-staged": "^15.5.1",
        "lodash": "^4.17.21",
        "lucide-react": "^0.482.0",
        "nanoid": "^5.1.5",
        "next-view-transitions": "^0.3.4",
        "nuqs": "^2.4.3",
        "postcss": "^8.5.3",
        "prettier": "3.5.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "prisma": "^6.6.0",
        "react-easy-crop": "^5.4.1",
        "react-leaflet": "^5.0.0",
        "semantic-release": "^24.2.3",
        "server-only": "^0.0.1",
        "stripe": "^17.7.0",
        "tailwind-merge": "^3.2.0",
        "tailwindcss": "^4.1.4",
        "tw-animate-css": "^1.2.5",
        "typescript": "^5.8.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.1.2",
        "zod": "^3.24.3",
        "zod-prisma-types": "^3.2.4",
        "zustand": "^5.0.3"
    },
    "lint-staged": {
        "*.{js,ts,tsx}": "pnpm lint:file",
        "*.{js,ts,tsx,json,css,md,yml,yaml}": "pnpm format:file"
    }
}
